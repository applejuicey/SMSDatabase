enum RoleType {
    ROOT,
    ADMIN,
    USER
}

type User {
    id: ID! @unique
    name: String
    email: String! @unique
    password: String!
    role: RoleType! @default(value: "USER")
}

type Compound {
    id: ID! @unique
    uniqueID: String! @unique
    commonName: String! @unique
    smiles: String! @unique
    chemicalFormula: String!
    structurePicAdd: String
    casCode: String @unique
    pubChemCID: String @unique
    chemSpiderID: String @unique
    msData: MSData!
    literatureSource: LiteratureSource!
}

type MSData {
    id: ID! @unique
    parentValue1: Float
    parentValue2: Float
    parentValue3: Float
    parentValue4: Float
    parentValue5: Float
    parentValue6: Float
    fragmentsValues1: [Float!]!
    fragmentsValues2: [Float!]!
    dataSource1: [String!]!
    referenceCodes1: [String!]!
    dataSource2: [String!]!
    referenceCodes2: [String!]!
    spectrumPicAddArray1: [String!]!
    spectrumPicAddArray2: [String!]!
}

type LiteratureSource {
    id: ID! @unique
    sourceCodes: [String!]!
}