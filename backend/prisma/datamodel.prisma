enum RoleType {
    ROOT,
    ADMIN,
    USER
}

type User {
    id: ID! @unique
    name: String
    email: String! @unique
    password: String!
    role: RoleType! @default(value: "USER")
}

type Compound {
    id: ID! @unique
    commonName: [String!]!
    casCode: String @unique
    pubChemCID: String @unique
    chemSpiderID: String @unique
    smiles: String! @unique
    chemicalFormula: String!
    msData: MSData!
    literatureSource: LiteratureSource!
}

type MSData {
    id: ID! @unique
    parentValue1: Float
    parentValue2: Float
    parentValue3: Float
    parentValue4: Float
    parentValue5: Float
    parentValue6: Float
    fragmentsValues: [Float!]!
    dataSource: [String!]!
    referenceCodes: [String!]!
}

type LiteratureSource {
    id: ID! @unique
    sourceCodes: [String!]!
}